   "id": "349d60bb",
   "metadata": {},
   "outputs": [],
   "source": [
    "def randoms(min, max):\n",
    "    while True:\n",
    "        yield random.randint(min, max)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "f0f87d7f",
   "metadata": {},
   "outputs": [],
   "source": [
